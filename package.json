{
  "name": "openapify",
  "version": "1.0.1",
  "description": "An Apify clone",
  "main": "out/index.js",
  "scripts": {
    "test": "cross-env DEBUG=openapify* jest --ci",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublish": "tsc"
  },
  "keywords": [],
  "author": "Tobias Salzmann",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^21.1.4",
    "jest": "^21.2.1",
    "ts-jest": "^21.1.3",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "@types/node": "^9.4.7",
    "@types/puppeteer": "^1.2.0",
    "cross-env": "^5.1.4",
    "node": "^9.8.0",
    "puppeteer": "0.13.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "mapCoverage": true,
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/out"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenApify/openapify.git"
  },
  "bugs": {
    "url": "https://github.com/OpenApify/openapify/issues"
  },
  "homepage": "https://github.com/OpenApify/openapify#readme"
}
